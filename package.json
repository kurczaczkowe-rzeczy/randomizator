  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "concurrently --kill-others \"react-scripts start\" \"npm run tsm\"",
    "start:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:ts": "eslint --ext=js,jsx,ts,tsx src",
    "lint:ts-build": "eslint --ext=js,jsx,ts,tsx --max-warnings 0 src",
    "tsm": "./node_modules/.bin/tsm src/ -w",
    "storybook": "start-storybook -p 6660 -s public --no-manager-cache",
    "build-storybook": "build-storybook -s public -o build.storybook",
    "bump": "npm version patch -m \"chore: Bump version to %s\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
